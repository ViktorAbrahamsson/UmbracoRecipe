{
  "name": "AbrahamssonRecipes",
  "version": "2.3.0",
  "targets": {
    "main": false,
    "css": {
      "source": "css/main.scss",
      "distDir": "wwwroot/",
      "includeNodeModules": true
    },
    "js": {
      "source": "js/main.ts",
      "distDir": "wwwroot/",
      "includeNodeModules": true
    }
  },
  "scripts": {
    "start": "parcel watch --no-cache",
    "watch": "parcel watch --no-cache",
    "dev": "concurrently \"dotnet watch\" \"parcel watch\"",
    "build": "parcel build --no-cache",
    "format": "pnpx prettier --write ./js/** && npx prettier --write ./css/**",
    "audit": "npm audit fix"
  },
  "devDependencies": {
    "@parcel/transformer-image": "^2.9.3",
    "@parcel/transformer-sass": "^2.9.3",
    "@parcel/transformer-typescript-types": "^2.9.3",
    "@types/alpinejs": "^3.7.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "alpinejs": "^3.13.5",
    "concurrently": "^7.6.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "mixitup": "^3.3.1",
    "parcel": "^2.9.3",
    "prettier": "^2.8.8",
    "typescript": "^4.9.5"
  },
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "@splidejs/splide": "^4.1.4"
  }
}
